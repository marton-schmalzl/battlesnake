{
  "family": "battlesnake",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "battlesnake",
      "image": "https://eu-north-1.console.aws.amazon.com/ecr/repositories/private/959078375082/battlesnake:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "taskRoleArn":  "${aws_iam_role.github-role.arn}"
}
